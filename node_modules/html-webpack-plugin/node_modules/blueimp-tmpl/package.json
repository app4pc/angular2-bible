    "jshint": "2.8.0",
    "mocha": "2.3.4",
    "uglify-js": "2.6.1"
  },
  "scripts": {
    "test": "jshint js test && mocha",
    "build": "uglifyjs js/tmpl.js -c -m > js/tmpl.min.js",
    "preversion": "npm test",
    "version": "npm run build && git add -A js",
    "postversion": "git push origin master --tags && npm publish"
  },
  "bin": {
    "tmpl.js": "js/compile.js"
  },
  "main": "js/tmpl.js",
  "jshintConfig": {
